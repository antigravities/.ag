#!/bin/bash
alias explorer='nautilus $@'
alias cp='cp -r'
alias e='nano $@'

function u(){
	curl https://raw.githubusercontent.com/antigravities/.ag/master/.ag > ~/.ag
	cd
	source .ag
}

function .ag(){
	if ! [ -d "~/ag" ]; then
		git clone "git@github.com:antigravities/.ag.git" ~/ag
	fi

	cd ~/ag
	cp ~/.ag .
	git commit -a --allow-empty-message -m ''
	git push
}

function eeti(){
	import /tmp/eeti.png
	echo -n "File uploaded to: ";
	curl -s -X POST -F file="@/tmp/eeti.png" -F user="$EETI_USERNAME" -F pass="$EETI_PASSWORD" "https://eeti.me/upload.php?uploaded" | tee >(xclip -selection clipboard)
	echo "";
}
